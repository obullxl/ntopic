<!-- 操作提示 -->
<script type="text/javascript" charset="utf-8" src="<%= static_host %>/public/js/atom-server.js"></script>
<div id="modal-progress" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="progress-header" aria-hidden="true">
	<div class="modal-header">
		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
		<h3 id="progress-header">操作处理中</h3>
	</div>
	<div class="modal-body">
		<div class="progress progress-striped active">
			<div class="bar" style="width: 100%;"></div>
		</div>
	</div>
</div>
<div id="modal-success" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="success-header" aria-hidden="true">
	<div class="modal-header">
		<h3 id="success-header">操作成功</h3>
	</div>
	<div class="modal-body">
		<div class="alert alert-success">
			<p>操作流水：<strong id="success-biz-log">123</strong></p>
		</div>
	</div>
	<div class="modal-footer">
		<button class="btn btn-success" data-dismiss="modal" aria-hidden="true">确定</button>
	</div>
</div>
<div id="modal-failure" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="failure-header" aria-hidden="true">
	<div class="modal-header">
		<h3 id="failure-header">操作失败</h3>
	</div>
	<div class="modal-body">
		<div class="alert alert-error alert-block">
			<p>操作流水：<strong id="failure-biz-log">123</strong></p>
			<p>错误代码：<strong id="failure-resp-code">123</strong></p>
			<p>错误描述：<strong id="failure-resp-desp">123</strong></p>
		</div>
	</div>
	<div class="modal-footer">
		<button class="btn btn-warning" data-dismiss="modal" aria-hidden="true">确定</button>
	</div>
</div>